{
	"name": "starwars-wiki",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier . -c",
		"format:fix": "prettier . -w",
		"prepare": "husky",
		"precommit": "lint-staged",
		"test": "vitest",
